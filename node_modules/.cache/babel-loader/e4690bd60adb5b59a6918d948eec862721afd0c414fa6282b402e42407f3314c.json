{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios'; // Import axios untuk melakukan HTTP requests\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      selectedTab: 'activities',\n      activities: [],\n      posts: [],\n      users: [],\n      newActivity: {\n        title: ''\n      }\n    };\n  },\n  methods: {\n    loadActivities() {\n      // Ambil data kegiatan dari JSONPlaceholder\n      axios.get('https://jsonplaceholder.typicode.com/todos').then(response => {\n        console.log('Data Kegiatan:', response.data);\n        // Assign data kegiatan ke dalam variabel activities\n        this.activities = response.data;\n      }).catch(error => {\n        console.error('Terjadi kesalahan saat mengambil data Kegiatan:', error);\n      });\n    },\n    loadPosts() {\n      // Ambil data postingan dari JSONPlaceholder\n      axios.get('https://jsonplaceholder.typicode.com/posts').then(response => {\n        console.log('Data Posts:', response.data);\n        // Assign data postingan ke dalam variabel posts\n        this.posts = response.data;\n      }).catch(error => {\n        console.error('Terjadi kesalahan saat mengambil data Posts:', error);\n      });\n\n      // Ambil data pengguna dari JSONPlaceholder\n      axios.get('https://jsonplaceholder.typicode.com/users').then(response => {\n        console.log('Data Pengguna:', response.data);\n        // Assign data pengguna ke dalam variabel users\n        this.users = response.data;\n      }).catch(error => {\n        console.error('Terjadi kesalahan saat mengambil data Pengguna:', error);\n      });\n    },\n    addActivity() {\n      if (this.newActivity.title) {\n        // Generate ID unik untuk kegiatan baru\n        const newActivityId = Math.floor(Math.random() * 1000) + 1;\n        // Tambahkan kegiatan baru ke dalam array activities\n        this.activities.push({\n          id: newActivityId,\n          title: this.newActivity.title\n        });\n        // Reset input judul kegiatan\n        this.newActivity.title = '';\n      } else {\n        alert('Judul kegiatan tidak boleh kosong!');\n      }\n    },\n    deleteActivity(activityId) {\n      // Hapus kegiatan dari array activities berdasarkan ID\n      this.activities = this.activities.filter(activity => activity.id !== activityId);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","selectedTab","activities","posts","users","newActivity","title","methods","loadActivities","get","then","response","console","log","catch","error","loadPosts","addActivity","newActivityId","Math","floor","random","push","id","alert","deleteActivity","activityId","filter","activity"],"sources":["C:\\Users\\IDEAPAD GAMING\\30-pbk-uts-223510056\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- Header -->\n    <div class=\"header\">\n      <button @click=\"selectedTab = 'activities'\" :class=\"{ active: selectedTab === 'activities' }\">Activities</button>\n      <button @click=\"selectedTab = 'posts'\" :class=\"{ active: selectedTab === 'posts' }\">Posts</button>\n    </div>\n    \n    <!-- Content -->\n    <div v-if=\"selectedTab === 'activities'\">\n      <div>\n        <h3>Tambah Kegiatan</h3>\n        <input type=\"text\" placeholder=\"Judul Kegiatan\" v-model=\"newActivity.title\">\n        <button @click=\"addActivity\">Tambah</button>\n      </div>\n      \n      <div v-if=\"activities.length > 0\">\n        <h2>Daftar Kegiatan</h2>\n        <ul>\n          <li v-for=\"activity in activities\" :key=\"activity.id\">\n            {{ activity.title }}\n            <button @click=\"deleteActivity(activity.id)\">Hapus</button>\n          </li>\n        </ul>\n      </div>\n      <div v-else>\n        <p>Belum ada kegiatan</p>\n      </div>\n    </div>\n\n    <div v-else-if=\"selectedTab === 'posts'\">\n      <div>\n        <h2>Daftar Pengguna</h2>\n        <ul>\n          <li v-for=\"user in users\" :key=\"user.id\">\n            {{ user.name }}\n          </li>\n        </ul>\n      </div>\n      <div>\n        <h2>Daftar Postingan</h2>\n        <ul>\n          <li v-for=\"post in posts\" :key=\"post.id\">\n            {{ post.title }}\n          </li>\n        </ul>\n      </div>\n    </div>\n\n    <div v-else>\n      <p>Tab belum dipilih</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'; // Import axios untuk melakukan HTTP requests\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      selectedTab: 'activities',\n      activities: [],\n      posts: [],\n      users: [],\n      newActivity: {\n        title: ''\n      }\n    };\n  },\n  methods: {\n    loadActivities() {\n      // Ambil data kegiatan dari JSONPlaceholder\n      axios.get('https://jsonplaceholder.typicode.com/todos')\n        .then(response => {\n          console.log('Data Kegiatan:', response.data);\n          // Assign data kegiatan ke dalam variabel activities\n          this.activities = response.data;\n        })\n        .catch(error => {\n          console.error('Terjadi kesalahan saat mengambil data Kegiatan:', error);\n        });\n    },\n    loadPosts() {\n      // Ambil data postingan dari JSONPlaceholder\n      axios.get('https://jsonplaceholder.typicode.com/posts')\n        .then(response => {\n          console.log('Data Posts:', response.data);\n          // Assign data postingan ke dalam variabel posts\n          this.posts = response.data;\n        })\n        .catch(error => {\n          console.error('Terjadi kesalahan saat mengambil data Posts:', error);\n        });\n        \n      // Ambil data pengguna dari JSONPlaceholder\n      axios.get('https://jsonplaceholder.typicode.com/users')\n        .then(response => {\n          console.log('Data Pengguna:', response.data);\n          // Assign data pengguna ke dalam variabel users\n          this.users = response.data;\n        })\n        .catch(error => {\n          console.error('Terjadi kesalahan saat mengambil data Pengguna:', error);\n        });\n    },\n    addActivity() {\n      if (this.newActivity.title) {\n        // Generate ID unik untuk kegiatan baru\n        const newActivityId = Math.floor(Math.random() * 1000) + 1;\n        // Tambahkan kegiatan baru ke dalam array activities\n        this.activities.push({\n          id: newActivityId,\n          title: this.newActivity.title\n        });\n        // Reset input judul kegiatan\n        this.newActivity.title = '';\n      } else {\n        alert('Judul kegiatan tidak boleh kosong!');\n      }\n    },\n    deleteActivity(activityId) {\n      // Hapus kegiatan dari array activities berdasarkan ID\n      this.activities = this.activities.filter(activity => activity.id !== activityId);\n    }\n  }\n};\n</script>\n\n<style>\n/* Gaya untuk aplikasi Vue Anda */\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n\n.header {\n  margin-bottom: 20px;\n}\n\n.header button {\n  padding: 10px 20px;\n  margin-right: 10px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  background-color: #007bff;\n  color: #fff;\n}\n</style>\n"],"mappings":";AAwDA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,YAAY;MACzBC,UAAU,EAAE,EAAE;MACdC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE;QACXC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf;MACAV,KAAK,CAACW,GAAG,CAAC,4CAA4C,EACnDC,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAEF,QAAQ,CAACX,IAAI,CAAC;QAC5C;QACA,IAAI,CAACE,UAAS,GAAIS,QAAQ,CAACX,IAAI;MACjC,CAAC,EACAc,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,iDAAiD,EAAEA,KAAK,CAAC;MACzE,CAAC,CAAC;IACN,CAAC;IACDC,SAASA,CAAA,EAAG;MACV;MACAlB,KAAK,CAACW,GAAG,CAAC,4CAA4C,EACnDC,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,aAAa,EAAEF,QAAQ,CAACX,IAAI,CAAC;QACzC;Q