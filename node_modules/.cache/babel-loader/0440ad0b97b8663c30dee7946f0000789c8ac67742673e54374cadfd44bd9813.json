{"ast":null,"code":"import Kegiatan from './components/Kegiatan.vue';\nexport default {\n  name: 'App',\n  components: {\n    Kegiatan\n  },\n  data() {\n    return {\n      users: [],\n      posts: []\n    };\n  },\n  created() {\n    this.fetchUsers();\n    this.fetchPosts();\n  },\n  methods: {\n    fetchUsers() {\n      fetch('https://jsonplaceholder.typicode.com/users').then(response => response.json()).then(data => {\n        this.users = data;\n      }).catch(error => {\n        console.error('Error fetching users:', error);\n      });\n    },\n    fetchPosts() {\n      fetch('https://jsonplaceholder.typicode.com/posts').then(response => response.json()).then(data => {\n        this.posts = data;\n      }).catch(error => {\n        console.error('Error fetching posts:', error);\n      });\n    }\n  },\n  watch: {\n    users: {\n      handler(newValue) {\n        console.log('Data pengguna telah berubah:', newValue);\n        // Lakukan sesuatu jika data pengguna berubah\n      },\n      deep: true\n    },\n    posts: {\n      handler(newValue) {\n        console.log('Data postingan telah berubah:', newValue);\n        // Lakukan sesuatu jika data postingan berubah\n      },\n      deep: true\n    }\n  }\n};","map":{"version":3,"names":["Kegiatan","name","components","data","users","posts","created","fetchUsers","fetchPosts","methods","fetch","then","response","json","catch","error","console","watch","handler","newValue","log","deep"],"sources":["C:\\Users\\IDEAPAD GAMING\\30-pbk-uts-223510056\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- Ganti konten yang ada dengan komponen Kegiatan -->\n    <Kegiatan :users=\"users\" :posts=\"posts\" />\n  </div>\n</template>\n\n<script>\nimport Kegiatan from './components/Kegiatan.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    Kegiatan\n  },\n  data() {\n    return {\n      users: [],\n      posts: []\n    };\n  },\n  created() {\n    this.fetchUsers();\n    this.fetchPosts();\n  },\n  methods: {\n    fetchUsers() {\n      fetch('https://jsonplaceholder.typicode.com/users')\n        .then(response => response.json())\n        .then(data => {\n          this.users = data;\n        })\n        .catch(error => {\n          console.error('Error fetching users:', error);\n        });\n    },\n    fetchPosts() {\n      fetch('https://jsonplaceholder.typicode.com/posts')\n        .then(response => response.json())\n        .then(data => {\n          this.posts = data;\n        })\n        .catch(error => {\n          console.error('Error fetching posts:', error);\n        });\n    }\n  },\n  watch: {\n    users: {\n      handler(newValue) {\n        console.log('Data pengguna telah berubah:', newValue);\n        // Lakukan sesuatu jika data pengguna berubah\n      },\n      deep: true\n    },\n    posts: {\n      handler(newValue) {\n        console.log('Data postingan telah berubah:', newValue);\n        // Lakukan sesuatu jika data postingan berubah\n      },\n      deep: true\n    }\n  }\n}\n</script>\n"],"mappings":"AAQA,OAAOA,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;IACjB,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPF,UAAUA,CAAA,EAAG;MACXG,KAAK,CAAC,4CAA4C,EAC/CC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACR,IAAG,IAAK;QACZ,IAAI,CAACC,KAAI,GAAID,IAAI;MACnB,CAAC,EACAW,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C,CAAC,CAAC;IACN,CAAC;IACDP,UAAUA,CAAA,EAAG;MACXE,KAAK,CAAC,4CAA4C,EAC/CC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACR,IAAG,IAAK;QACZ,IAAI,CAACE,KAAI,GAAIF,IAAI;MACnB,CAAC,EACAW,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C,CAAC,CAAC;IACN;EACF,CAAC;EACDE,KAAK,EAAE;IACLb,KAAK,EAAE;MACLc,OAAOA,CAACC,QAAQ,EAAE;QAChBH,OAAO,CAACI,GAAG,CAAC,8BAA8B,E