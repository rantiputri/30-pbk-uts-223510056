{"ast":null,"code":"import Kegiatan from './components/Kegiatan.vue';\nexport default {\n  name: 'App',\n  components: {\n    Kegiatan\n  },\n  data() {\n    return {\n      todos: [],\n      posts: [],\n      currentData: [] // Menyimpan data yang sedang ditampilkan\n    };\n  },\n  created() {\n    this.fetchData('todos'); // Mulai dengan menampilkan data todos saat aplikasi dimuat\n  },\n  methods: {\n    fetchData(type) {\n      fetch(`https://jsonplaceholder.typicode.com/${type}`).then(response => response.json()).then(data => {\n        this[type] = data;\n        if (type === 'todos') {\n          this.currentData = this.todos;\n        } else {\n          this.currentData = this.posts;\n        }\n      }).catch(error => {\n        console.error(`Error fetching ${type}:`, error);\n      });\n    },\n    toggleData(type) {\n      this.currentData = []; // Kosongkan data yang ditampilkan\n      this.fetchData(type); // Ambil data baru berdasarkan jenisnya\n    }\n  }\n};","map":{"version":3,"names":["Kegiatan","name","components","data","todos","posts","currentData","created","fetchData","methods","type","fetch","then","response","json","catch","error","console","toggleData"],"sources":["C:\\Users\\IDEAPAD GAMING\\30-pbk-uts-223510056\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <button @click=\"toggleData('todos')\">Todos</button>\n    <button @click=\"toggleData('posts')\">Posts</button>\n    <!-- Ganti konten yang ada dengan komponen Kegiatan -->\n    <Kegiatan :data=\"currentData\" />\n  </div>\n</template>\n\n<script>\nimport Kegiatan from './components/Kegiatan.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    Kegiatan\n  },\n  data() {\n    return {\n      todos: [],\n      posts: [],\n      currentData: [] // Menyimpan data yang sedang ditampilkan\n    };\n  },\n  created() {\n    this.fetchData('todos'); // Mulai dengan menampilkan data todos saat aplikasi dimuat\n  },\n  methods: {\n    fetchData(type) {\n      fetch(`https://jsonplaceholder.typicode.com/${type}`)\n        .then(response => response.json())\n        .then(data => {\n          this[type] = data;\n          if (type === 'todos') {\n            this.currentData = this.todos;\n          } else {\n            this.currentData = this.posts;\n          }\n        })\n        .catch(error => {\n          console.error(`Error fetching ${type}:`, error);\n        });\n    },\n    toggleData(type) {\n      this.currentData = []; // Kosongkan data yang ditampilkan\n      this.fetchData(type); // Ambil data baru berdasarkan jenisnya\n    }\n  }\n}\n</script>\n"],"mappings":"AAUA,OAAOA,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAC,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,OAAO,CAAC,EAAE;EAC3B,CAAC;EACDC,OAAO,EAAE;IACPD,SAASA,CAACE,IAAI,EAAE;MACdC,KAAK,CAAE,wCAAuCD,IAAK,EAAC,EACjDE,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACT,IAAG,IAAK;QACZ,IAAI,CAACO,IAAI,IAAIP,IAAI;QACjB,IAAIO,IAAG,KAAM,OAAO,EAAE;UACpB,IAAI,CAACJ,WAAU,GAAI,IAAI,CAACF,KAAK;QAC/B,OAAO;UACL,IAAI,CAACE,WAAU,GAAI,IAAI,CAACD,KAAK;QAC/B;MACF,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAE,kBAAiBN,IAAK,GAAE,EAAEM,KAAK,CAAC;MACjD,CAAC,CAAC;IACN,CAAC;IACDE,UAAUA,CAACR,IAAI,EAAE;MACf,IAAI,CAACJ,WAAU,GAAI,EAAE,EAAE;MACvB,IAAI,CAACE,SAAS,CAACE,IAAI,CAAC,EAAE;IACxB;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}